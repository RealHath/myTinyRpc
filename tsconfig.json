{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es2020",
    "sourceMap": true,
    "removeComments": true,
    "outDir": "publish",
    "esModuleInterop": false,
    "diagnostics": true,
    "noEmitOnError": true,
    "noEmitHelpers": true,
    // "listEmittedFiles": true,
    // "listFiles": true,
    "preserveSymlinks": true,
    "strict": true,
    "noImplicitAny": true,
    "strictNullChecks": false,
    "strictFunctionTypes": true,
    "strictPropertyInitialization": false,
    "noImplicitThis": true,
    "noImplicitReturns": true,
    "alwaysStrict": true,
  },
  "exclude": [
    "node_modules",
  ],
  "include": [
    "src/*",
    "src/**/*"
  ]
}